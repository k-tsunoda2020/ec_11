<p id="notice"><%= notice %></p><!-- 他の場所から遷移してきたときのメッセージ用 -->
<h1>Articles</h1>

<table>
    <thead>
        <tr><th>Title</th><th>Content</th></tr>
    </thead>
    <tbody>
        <% @articles.each do |article| %>
            <tr><th><%= article.title %></th><th><%= article.content %></th></tr>
        <% end %> 
    </tbody>
</table>

<%= link_to 'New Article', new_article_path %>